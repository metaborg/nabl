module test

signature
  constraints
    concatF : list(string) * list(string) -> list(string)

rules
  
  concatF([], ys) = ys.
  concatF([x|xs], ys) = [x|concatF(xs, ys)].

signature
  constraints
    concatP : list(string) * list(string) * list(string)

rules
  
  concatP([], ys, ys).
  concatP([x|xs], ys, [x|zs]) :- concatP(xs, ys, zs).

